<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gjyxfs.dao.IUserDAO" >
	<resultMap id="BaseResultMap" type="com.gjyxfs.model.User" >
		<id column="user_id" property="userID" />
		<result column="user_name" property="userName" />
		<result column="user_password" property="userPassword" />
		<result column="phone_number" property="phoneNumber" />
		<result column="expired_time" property="expiredTime" />
		<result column="group_id" property="groupID" />
	</resultMap>
	
	<resultMap type="java.lang.String" id="allUsernameList" />
	
	<sql id="Base_Column_List" >
    	user_id, user_name, user_password, phone_number, expired_time, group_id
	</sql>

	<select id="selectAll" resultMap="BaseResultMap">
		select  user_id, user_name, user_password, phone_number, expired_time,group_id from user
	</select>

</mapper>